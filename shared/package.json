{
  "name": "daohq-shared",
  "version": "1.0.7",
  "description": "DAO HQ scenes shared codebase",
  "scripts": {
    "prestart": "git rev-parse HEAD > version.txt",
    "start": "sdk-commands start",
    "predeploy": "git rev-parse HEAD > version.txt",
    "deploy": "sdk-commands deploy",
    "build": "sdk-commands build",
    "upgrade-sdk": "npm install --save-dev @dcl/sdk@latest",
    "upgrade-sdk:next": "npm install --save-dev @dcl/sdk@next"
  },
  "devDependencies": {
    "tsx": "^4.7.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=6.0.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 120,
    "trailingComma": "none"
  },
  "dependencies": {
    "@dcl-sdk/utils": "^1.2.5",
    "@dcl/schemas": "^12.0.0"
  },
  "bundleDependencies": [
    "@dcl-sdk/utils"
  ]
}
